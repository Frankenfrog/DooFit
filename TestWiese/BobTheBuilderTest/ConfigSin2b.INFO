  }  
}

PdfFull "pdfFull"
{
  desc        "P"
  Extended    false 
  PdfsForCategory "catTrigTrag"
  {
    "{B;t}" "pdfFullBt"
    {
      desc        "P_{B,t}"
      Extended    false
      Components
      {
        Sig "pdfSig"
        { 
          desc          "P_{Sig}"
          Extended      true
          Dimensions
          {
            varMass     "Gaussian"
            varMassDau1 "Gaussian"
          }
        }
        Pbg "pdfPbg"
        {
          varMass     "pdfBtPbgMass"
          {
            desc  "juju"
            type  "Gaussian"
            mean  "parBtSigMassMean,m_{B}"
            sigma "parBtSigMassSigma,#sigma_{m,B}"
            CondPdf   ""
            CondVars  ""
            Acc       ""
          }
          varMassDau1 "Gaussian"
        }
        Lbg "pdfLbg"
        {
          varMass     ""
          vasMassDau1 "=Signal"
        }
      }
    }
  }
} 


